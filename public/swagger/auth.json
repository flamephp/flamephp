{
    "openapi": "3.0.0",
    "info": {
        "title": "API文档",
        "description": "用户认证平台",
        "contact": {
            "name": "API Develop Team"
        },
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://flame.test/",
            "description": "开发环境"
        }
    ],
    "paths": {
        "/auth/login/mobile": {
            "post": {
                "tags": [
                    "登录"
                ],
                "summary": "通过手机号码登录",
                "operationId": "79fbd311bbea51c3829215c420a0d5c9",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/LoginMobileRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/LoginResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "UserSchema": {
                "properties": {
                    "id": {
                        "description": "",
                        "type": "integer"
                    },
                    "name": {
                        "description": "昵称",
                        "type": "string"
                    },
                    "avatar": {
                        "description": "头像",
                        "type": "string"
                    },
                    "birthday": {
                        "description": "生日",
                        "type": "string"
                    },
                    "mobile": {
                        "description": "手机号码",
                        "type": "string"
                    },
                    "mobile_verified_at": {
                        "description": "手机号码验证时间",
                        "type": "string"
                    },
                    "password": {
                        "description": "登录密码",
                        "type": "string"
                    },
                    "status": {
                        "description": "状态:1正常;2禁用",
                        "type": "integer"
                    },
                    "remember_token": {
                        "description": "",
                        "type": "string"
                    },
                    "created_at": {
                        "description": "",
                        "type": "string"
                    },
                    "updated_at": {
                        "description": "",
                        "type": "string"
                    },
                    "deleted_at": {
                        "description": "",
                        "type": "string"
                    }
                },
                "type": "object"
            },
            "LoginMobileRequest": {
                "required": [
                    "mobile",
                    "password",
                    "captcha",
                    "uuid"
                ],
                "properties": {
                    "mobile": {
                        "description": "手机号码",
                        "type": "string",
                        "example": "13901889999"
                    },
                    "password": {
                        "description": "登录密码",
                        "type": "string",
                        "example": "123456aA"
                    },
                    "captcha": {
                        "description": "图片验证码",
                        "type": "string",
                        "example": "1234"
                    },
                    "uuid": {
                        "description": "图片验证码UUID参数",
                        "type": "string",
                        "example": "1234"
                    }
                },
                "type": "object"
            },
            "LoginResponse": {
                "properties": {
                    "jwt": {
                        "description": "用户JSON Web Token凭证",
                        "type": "string",
                        "example": "123456"
                    }
                },
                "type": "object"
            }
        }
    }
}