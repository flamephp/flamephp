                    "image": {
                        "description": "文章海报",
                        "type": "string"
                    },
                    "content": {
                        "description": "文章描述",
                        "type": "string"
                    },
                    "status": {
                        "description": "状态（1正常，2禁用）",
                        "type": "integer"
                    },
