                "required": [
                    "title"
                ],
                "properties": {
                    "title": {
                        "description": "文章标题",
                        "type": "string",
                        "example": "untitle"
                    }
                },
                "type": "object"
            },
            "ArticleUpdateRequest": {
                "required": [
                    "id",
                    "title"
                ],
                "properties": {
                    "id": {
                        "description": "文章ID",
                        "type": "integer",
                        "example": 1
                    },
                    "title": {
                        "description": "文章标题",
                        "type": "string",
                        "example": "untitle"
                    }
                },
                "type": "object"
            },
            "ArticleResponse": {
                "properties": {
                    "id": {
