                "tags": [
                    "文章管理"
                ],
                "summary": "创建文章接口",
                "operationId": "5c54accf75bc55851669c5afc81acf24",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/ArticleCreateRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ArticleResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/manager/user": {
            "post": {
                "tags": [
                    "用户管理"
                ],
                "summary": "用户列表",
                "operationId": "b9c71369a9403c466cd8f9ee002404d6",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "当前页码",
                        "required": true,
                        "example": 1
                    },
                    {
                        "name": "pageSize",
                        "in": "query",
                        "description": "每页分页数",
                        "required": false,
                        "example": 10
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/UserQueryRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/UserResponse"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        }
    },
    "components": {
